graphy>\n                </Grid>\n                <Grid item>\n                    <Grid container alignItems='flex-end'>\n                        <AccountCircleIcon fontSize='large' />\n                        <Typography className={classes.author}>By <span style={{ fontWeight: 'bold' }}>{recipeDetail.postedBy.name}</span></Typography>\n                    </Grid>\n                </Grid>\n                {recipeDetail.images.length > 0 && <Grid item>\n                       <ImageGallery images={recipeDetail.images} />\n                </Grid>}\n               \n                <Grid item >\n                    <Ingredient ingredients={recipeDetail.ingredients} />\n                </Grid>\n                <Divider/>\n                <Direction directions={recipeDetail.directions}/>\n                <Divider/>\n                <CommentSection comments={recipeDetail.comments} id={recipeDetail.id}/>\n            </Grid>\n        </Grid>\n    )\n}\n\nexport default withRouter(withStyles(styles)(RecipeDetailPage));","/Users/abhishekkumarjha/Documents/react/recipeshop/src/pages/RecipeDetailPage/style.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/ImageGallery/index.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/ImageGallery/style.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/Ingredient/index.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/Ingredient/style.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/Direction/index.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/Direction/style.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/CommentSection/index.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/components/CommentSection/style.js",[],"/Users/abhishekkumarjha/Documents/react/recipeshop/src/styled-components/Link.js",["181","182"],"import React,{Component} from 'react';\nimp